{
  "name": "vuexblaze",
  "description": "Yet another Firebase bindings for Vuex",
  "version": "0.0.8",
  "author": "Yoshinori Ishii <yoshinori.ishii@gmail.com>",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "lodash": "^4.17.11",
    "pluralize": "^7.0.0",
    "promise-queue": "^2.2.5"
  },
  "main": "dist/cjs/index.js",
  "module": "src/index.js",
  "files": [
    "src",
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "babel src --out-dir dist/cjs",
    "prepublishOnly": " npm run build",
    "lint": "eslint --ext .js ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ishiijp/vuexblaze.git"
  },
  "keywords": [
    "vuex",
    "firestore",
    "firebase",
    "google",
    "database",
    "bind"
  ],
  "peerDependencies": {
    "firebase": ">= 5.0.0"
  },
  "bugs": {
    "url": "https://github.com/ishiijp/vuexblaze/issues"
  },
  "homepage": "https://github.com/ishiijp/vuexblaze#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "eslint": "^5.10.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-config-recommended": "^4.0.0",
    "eslint-plugin-prettier": "^3.0.0"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  }
}
